{
    "coc.preferences.formatOnSaveFiletypes": ["lua", "python", "rust"],
    "coc.source.around.priority": 1,
    "coc.source.buffer.priority": 0,
    "codeLens.enable": false,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic-languageserver.filetypes": {
        "bash": "shellcheck",
        "sh": "shellcheck",
        "zsh": "shellcheck",
        "vim": "vint"
    },
    "git.addGBlameToBufferVar": true,
    "git.addGBlameToVirtualText": true,
    "git.showCommitInFloating": true,
    "javascript.implementationsCodeLens.enable": false,
    "javascript.referencesCodeLens.enable": false,
    "Lua.diagnostics.globals": ["vim"],
    "pyright.enable": true,
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pytypeEnabled": true,
    // https://github.com/fannheyward/coc-rust-analyzer/blob/master/package.json#L69
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.checkOnSave.extraArgs": ["--", "-Wclippy::pedantic", "-Wclippy::all"],
    "rust-analyzer.imports.merge.glob": false,
    "rust-analyzer.imports.prefix": "crate",
    "rust-analyzer.inlayHints.bindingModeHints.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.minLines": 11,
    "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
    "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
    "rust-analyzer.inlayHints.expressionAdjustmentHints.mode": "postfix",
    "rust-analyzer.inlayHints.discriminantHints.enable": "always",
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "rust-analyzer.lens.location": "above_whole_item",
    "rust-analyzer.lens.references.adt.enable": true,
    "rust-analyzer.lens.references.enumVariant.enable": true,
    "rust-analyzer.lens.references.method.enable": true,
    "rust-analyzer.lens.references.trait.enable": true,
    "rust-analyzer.restartServerOnConfigChange": true,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "rust-analyzer.updates.channel": "nightly",
    "rust-analyzer.updates.prompt": false,
    "suggest.noselect": true,
    "suggest.removeDuplicateItems": true,
    "tsserver.formatOnType": true,
    "typescript.implementationsCodeLens.enable": false,
    "typescript.referencesCodeLens.enable": false
}
