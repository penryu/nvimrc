{
    "coc.preferences.formatOnSaveFiletypes": ["lua", "python", "rust"],
    "coc.source.around.priority": 1,
    "coc.source.buffer.priority": 0,
    "codeLens.enable": false,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic-languageserver.filetypes": {
        "bash": "shellcheck",
        "sh": "shellcheck",
        "zsh": "shellcheck",
        "vim": "vint"
    },
    "git.addGBlameToBufferVar": true,
    "git.addGBlameToVirtualText": true,
    "git.showCommitInFloating": true,
    "javascript.implementationsCodeLens.enable": false,
    "javascript.referencesCodeLens.enable": false,
    "pyright.enable": true,
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pytypeEnabled": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.diagnostics.enable": true,
    "rust-analyzer.highlightRelated.exitPoints.enable": true,
    "rust-analyzer.highlightRelated.yieldPoints.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": true,
    "rust-analyzer.inlayHints.reborrowHints.enable": "always",
    "rust-analyzer.inlayHints.renderColons": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.lens.debug.enable": true,
    "rust-analyzer.rustfmt.rangeFormatting.enable": true,
    "rust-analyzer.updates.channel": "nightly",
    "suggest.noselect": true,
    "suggest.removeDuplicateItems": true,
    "tsserver.formatOnType": true,
    "typescript.implementationsCodeLens.enable": false,
    "typescript.referencesCodeLens.enable": false
}
